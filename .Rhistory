data2016 %>%
mutate(BASES =
paste(ifelse(BASE1_RUN_ID > '',1,0),
ifelse(BASE2_RUN_ID > '',1,0),
ifelse(BASE3_RUN_ID > '',1,0), sep = ""),
STATE = paste(BASES, OUTS_CT)) ->
data2016
